<!-- If the the start_date is present on the params, then update the calendar partial -->
<!-- otherwise add more items to the feed -->
<% if params["start_date"].present? %>
  $("#events_calendar").html("<%= j render partial: "calendar" %>");

<% else %>
  $("#items-list").append('<%= j render partial: "feed_item", collection: @feed_items %>');

  <% if @feed_items.next_page %>
    $('.pagination').replaceWith('<%= j will_paginate(@feed_items) %>');
  <% end %>
<% end %>
